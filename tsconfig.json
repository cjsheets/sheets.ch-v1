{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "Node",
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "pretty": true,
    "removeComments": false,
    "typeRoots": [
      "@types",
      "node_modules/@types"
    ],
  },
  "moduleResolution": "node",
  "include": [
    "src",
    "@types",
    "node_modules/@types"
  ]
}