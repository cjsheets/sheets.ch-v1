{
  "name": "using-typescript",
  "version": "1.0.0",
  "description": "Gatsby example site demonstrating how to use Typescript with Gatsby",
  "author": "fabien0102 <fabien0102@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "build:dev": "gatsby develop",
    "pretty": "prettier --config .prettierrc --write \"{gatsby-*.js,src/**/*.tsx}\"",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./@types/graphql-types.d.ts"
  },
  "dependencies": {
    "gatsby": "next",
    "gatsby-link": "next",
    "gatsby-plugin-feed": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sass": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-typescript": "next",
    "gatsby-remark-copy-linked-files": "next",
    "gatsby-remark-images": "next",
    "gatsby-remark-prismjs": "next",
    "gatsby-remark-responsive-iframe": "next",
    "gatsby-remark-smartypants": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "graphql-code-generator": "^0.9.4",
    "graphql-codegen-typescript-template": "^0.9.4",
    "lodash": "^4.17.5",
    "node-sass": "^4.9.0",
    "postcss-scss": "^1.0.6",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.110",
    "@types/node": "^10.3.5",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-helmet": "^5.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/react-router": "^4.0.27",
    "@types/react-router-dom": "^4.2.7",
    "prettier": "^1.13.5",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  }
}
